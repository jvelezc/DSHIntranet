<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> 
<!--Get some images at: http://dmhinsite.ca.gov/LibraryReferences/LogoManual.asp-->
<html lang="en">
    <head>
        <meta charset="utf-8">
        <!--[if IEMobile]>  <meta http-equiv="cleartype" content="on">  <![endif]-->
       @*
          This: <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
            I moved the above line to the web config. To pass w3c validation
        *@
            

        <title>@ViewBag.Title - Department of State Hospitals Intranet</title>
        <meta name="description" content="@ViewBag.Description Department of State Hospitals Intranet">
        <meta name="Keywords" content="@ViewBag.KeyWords ,Department of State Hospitals Intranet" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
       
       @Styles.Render(
            "~/Content/boilerplate-css", 
            "~/Content/themes/base/css", 
            "~/Content/css"
        )
        @Scripts.Render("~/bundles/modernizr")
        
        <!--Specific to DSH starts here--> 
        <link href="~/Content/themes/cagov/style.css" rel="stylesheet" />
        <link href="~/Content/themes/cagov/600.css" rel="stylesheet" />
        <!--Specific to DSH ends here-->

    </head>

    <body class="group home  ">
        <!--TODO write documentation of usage of _Menu--> 
        @Html.Partial("_Menu")
        @RenderBody()
    <footer>

     @Html.Partial("_Footer")


      @Scripts.Render(
        "~/bundles/jquery",
        "~/bundles/jqueryui",
        "~/bundles/jqueryval",
        "~/bundles/jsextlibs" ,
        "~/bundles/cagovscripts"
       )
    
        @RenderSection("scripts", required: false)

      </footer>    
      <div id="background_decorative">&nbsp;</div>
    </body>
</html>
