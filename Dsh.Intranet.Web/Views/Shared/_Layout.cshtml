<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>@ViewBag.Title - Department of State Hospitals Intranet</title>
        <meta name="description" content="@ViewBag.Description">
        <meta name="viewport" content="width=device-width">
       
         @Styles.Render(
            "~/Content/boilerplate-css", 
            "~/Content/themes/base/css", 
            "~/Content/css"
        )
        @Scripts.Render("~/bundles/modernizr")
   
    </head>

    <body>
        <!--TODO write documentation of usage of RenderBody--> 
        @Html.Partial("_Menu")
        @RenderBody()
    <footer>

      @Html.Partial("_Footer")
      @Scripts.Render(
        "~/bundles/jquery",
        "~/bundles/jqueryui",
        "~/bundles/jqueryval",
        "~/bundles/jsextlibs"  
       )
        <!--Please see documentation for usage --> 
        <!--TODO write documentation for scripts section--> 
       @RenderSection("scripts", required: false)
        
      </footer>    

    </body>
</html>
